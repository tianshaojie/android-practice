<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yuzhi.fine">

    <permission
        android:name="com.yuzhi.fine.aidl.permission.SERVICE_PERMISSION"
        android:protectionLevel="normal" />

    <application
        android:name="com.yuzhi.fine.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yuzhi.fine.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.yuzhi.fine.activity.LazyLoadFragmentActivity"
            android:screenOrientation="portrait"
            android:label="Fragment 懒加载"/>

        <service
            android:name="com.yuzhi.fine.service.WebViewAidlService"
            android:exported="true"
            android:permission="com.yuzhi.fine.aidl.permission.SERVICE_PERMISSION">
            <intent-filter>
                <action android:name="com.yuzhi.fine.library.web.IWebAidlInterface" />
            </intent-filter>
        </service>


    </application>

</manifest>