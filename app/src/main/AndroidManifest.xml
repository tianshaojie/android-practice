<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.skyui">

    <uses-permission android:name="cn.skyui.aidl.permission.SERVICE_PERMISSION" />

    <application
        android:name="cn.skyui.practice.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        tools:replace="android:theme"
        android:theme="@style/AppTheme">
        <activity android:name="cn.skyui.practice.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="cn.skyui.practice.activity.LazyLoadFragmentActivity"
            android:screenOrientation="portrait"
            android:label="Fragment 懒加载"/>

        <service
            android:name="cn.skyui.library.web.service.WebViewPreLoadService"
            android:exported="true"
            android:permission="cn.skyui.aidl.permission.SERVICE_PERMISSION">
            <intent-filter>
                <action android:name="cn.skyui.library.web.IWebAidlInterface" />
            </intent-filter>
        </service>


    </application>

</manifest>